{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "npm: watch",
			"type": "npm",
			"script": "watch",
			"problemMatcher": "$tsc-watch",
			"isBackground": true,
			"presentation": {
				"reveal": "never"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "npm: compile",
			"type": "npm",
			"script": "compile",
			"problemMatcher": "$tsc",
			"group": "build"
		},
		{
			"label": "npm: pretest",
			"type": "npm",
			"script": "pretest",
			"dependsOrder": "sequence",
			"group": "test"
		},
		{
			"label": "npm: test",
			"type": "npm",
			"script": "test",
			"dependsOn": "npm: pretest",
			"problemMatcher": "$tsc",
			"group": {
				"kind": "test",
				"isDefault": true
			}
		},
		{
			"label": "npm: package",
			"type": "npm",
			"script": "package",
			"dependsOn": "npm: compile",
			"group": "build"
		}
	]
}